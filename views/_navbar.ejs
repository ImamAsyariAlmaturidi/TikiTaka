<header class="flex justify-evenly mx-10 mt-4 border-b-2 h-14 items-center w-full bg-gray-300">
    <h1 class="text-black tracking-widest font-bold text-1xl">TIKITAKA</h1>
    <form method="get" action="/landing/<%= profile.Profile.id %>">
        <div class="flex">
            <input class="border rounded-l-lg py-2 px-2" type="text" name="title" placeholder="Search...">
            <button type="submit" class="bg-black hover:bg-gray-400 hover:text-black text-white font-bold py-2 px-4 rounded-r-lg ml-1">
                Search
            </button>
        </div>            
    </form>
    <span class="flex"> 
        <span>
            <a href="/users/logout/<%= profile.Profile.id %>" class="font-bold text-lg tracking-widest"><button>Logout</button></a>
        </span>
    </span> 
</header>